   <string notr="true">/*----------------------------- MainWindow -----------------------------------------*/
#mwDashBoard {
	
	background-color: rgb(52, 73, 94);
}


#scaTelas, #scrollArea {
	background-color: transparent;
}

/*---------------------------- Frame ------------------------------------------*/
#frMain{
	background-color: transparent;
	border-radius: 8px;
}

/*---------------------------- Push Buttons ------------------------------------------*/

#pbConfig, #pbHome, 
#pbInfoGerais, #pbFerramentas {
	background-color: rgb(41, 128, 185);
	border-radius: 20px;
}

#pbConfig:hover, 
#pbHome:hover, 
#pbInfoGerais:hover,
#pbFerramentas:hover {
	background-color: rgba(190, 250, 245, 190);
	border-radius: 20px;
}

#pbConfig {
	background-image: url(:/configuracoes/settings.png);
	background-repeat: no-repeat;
	background-position: center;
}

#pbHome {
	background-image: url(:/home/home.png);
	background-repeat: no-repeat;
	background-position: center;
}

#pbInfoGerais {
	background-image: url(:/sobre/information.png);
	background-repeat: no-repeat;
	background-position: center;
}

#pbFerramentas {
	background-image: url(:/ferramentas/ferramentas.png);
	background-repeat: no-repeat;
	background-position: center;
}

/*---------------------------- StackWidget ------------------------------------------*/

#stkMainDashBoard{
	background-color: white;
	border-radius: 8px;
}</string>
