      <property name="styleSheet">
       <string notr="true">/*-------------------------------- ScrollArea -----------------------------------------*/
#scaCadastro, #scrollArea {
	background-color: transparent;
}

/*-------------------------------- Labels -----------------------------------------*/
#lbTitulo{
	font-family: &quot;TeX Gyre Adventor&quot;;
	font-size: 24px;
}

#lbDescricaoTela, 
#lbFrTituloInfoPessoal,
#lbFrTituloInfoProfissional,
#lbFrTituloInfoResidencial,
#lbFrTituloInfoBancarias {
	font-family: &quot;TeX Gyre Adventor&quot;;
	font-size: 14px;

	background-color: white;
}

#lbInfoCnis {
	font-family: &quot;TeX Gyre Adventor&quot;;
	font-size: 12px;	
}

QFrame &gt; QLabel {
	font-family: &quot;TeX Gyre Adventor&quot;;
	font-size: 12px;	
}



/*---------------------------------- Frames -----------------------------------*/
#frInfoPessoais, #frInfoProfissional, 
#frInfoResidencial, #frInfoBancarias {
	border-radius: 4px;
	border: 1px solid rgb(85, 87, 83);
}

/*---------------------------------- CheckBox -----------------------------------*/
#cbClienteAntigo {
	font-family: &quot;TeX Gyre Adventor&quot;;
	font-size: 12px;
}

/*------------------------------- Push Buttons ------------------------------*/

#pbCarregaCnis {
	font-family: &quot;TeX Gyre Adventor&quot;;
	font-size: 12px;
	color: white;

	border-radius: 4px;
/*	background-color: rgb(52, 73, 94);*/
	background-color: rgb(92, 53, 102);
}

#pbCarregaCnis:hover {
	font-family: &quot;TeX Gyre Adventor&quot;;
	font-size: 12px;
	color: white;

	border-radius: 4px;
	background-color: rgb(72, 93, 114);
